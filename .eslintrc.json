{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {
    "no-new": 0,
    "no-underscore-dangle": "off",
    "no-tabs": ["error", { "allowIndentationTabs": true }],
    "no-mixed-operators": 0,
    "no-mixed-spaces-and-tabs": 0,
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ]
  }
}